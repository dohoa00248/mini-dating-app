# Server Configuration
PORT=3000
NODE_ENV=development

# Session Configuration
# A random string used to sign the session ID cookie
SESSION_SECRET=your_random_secret_key_here

# MongoDB Configuration
# Local example: mongodb://localhost:27017/heart_sync
# Atlas example: mongodb+srv://<user>:<password>@cluster.mongodb.net/heart_sync
DB_MONGODB_URI=your_mongodb_connection_string_here